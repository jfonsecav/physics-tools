      "source": [
        "import matplotlib.pyplot as plt\n",
        "from lxml import etree\n",
        "\n",
        "# Ruta al archivo (en la misma carpeta que el script)\n",
        "file_path = \"ZnO sin backgroud.XRDML\"\n",
        "\n",
        "# --- Lectura del archivo .XRDML ---\n",
        "tree = etree.parse(file_path)\n",
        "root = tree.getroot()\n",
        "ns = {\"x\": root.nsmap[None]}  # namespace por defecto\n",
        "\n",
        "# Extraer intensidades\n",
        "intensity_elements = root.xpath('.//x:intensities', namespaces=ns)\n",
        "intensities = list(map(float, intensity_elements[0].text.strip().split()))\n",
        "\n",
        "# Extraer ángulos de inicio y fin\n",
        "start_angle = float(root.xpath('.//x:startPosition', namespaces=ns)[0].text)\n",
        "end_angle = float(root.xpath('.//x:endPosition', namespaces=ns)[0].text)\n",
        "\n",
        "# Calcular ángulos\n",
        "num_points = len(intensities)\n",
        "angles = [\n",
        "    start_angle + i * (end_angle - start_angle) / (num_points - 1)\n",
        "    for i in range(num_points)\n",
        "]\n",
        "\n",
        "# --- Gráfica tipo artículo científico ---\n",
        "plt.figure(figsize=(8, 5))\n",
        "plt.plot(angles, intensities, color='black', linewidth=1)\n",
        "\n",
        "plt.title(\"\", fontsize=14, weight='bold')\n",
        "plt.xlabel(r'2$\\theta$ (°)', fontsize=16)\n",
        "plt.ylabel('Intensidad (u.a.)', fontsize=16)\n",
        "plt.tight_layout()\n",
        "plt.xlim(min(angles), max(angles))\n",
        "\n",
        "# Estética mejorada\n",
        "plt.xticks(fontsize=15)\n",
        "plt.yticks(fontsize=15)\n",
        "ax = plt.gca()\n",
        "ax.spines['top'].set_visible(False)\n",
        "ax.spines['right'].set_visible(False)\n",
        "\n",
        "# Mostrar la gráfica\n",
        "plt.show()\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "from lxml import etree\n",
        "\n",
        "# Ruta al archivo (en la misma carpeta que el script)\n",
        "file_path = \"ZnO-Co-backoff.XRDML\"\n",
        "\n",
        "# --- Lectura del archivo .XRDML ---\n",
        "tree = etree.parse(file_path)\n",
        "root = tree.getroot()\n",
        "ns = {\"x\": root.nsmap[None]}  # namespace por defecto\n",
        "\n",
        "# Extraer intensidades\n",
        "intensity_elements = root.xpath('.//x:intensities', namespaces=ns)\n",
        "intensities = list(map(float, intensity_elements[0].text.strip().split()))\n",
        "\n",
        "# Extraer ángulos de inicio y fin\n",
        "start_angle = float(root.xpath('.//x:startPosition', namespaces=ns)[0].text)\n",
        "end_angle = float(root.xpath('.//x:endPosition', namespaces=ns)[0].text)\n",
        "\n",
        "# Calcular ángulos\n",
        "num_points = len(intensities)\n",
        "angles = [\n",
        "    start_angle + i * (end_angle - start_angle) / (num_points - 1)\n",
        "    for i in range(num_points)\n",
        "]\n",
        "\n",
        "# --- Gráfica tipo artículo científico ---\n",
        "plt.figure(figsize=(8, 5))\n",
        "plt.plot(angles, intensities, color='black', linewidth=1)\n",
        "\n",
        "plt.xlabel(r'2$\\theta$ (°)', fontsize=16)\n",
        "plt.ylabel('Intensidad (u.a.)', fontsize=16)\n",
        "plt.tight_layout()\n",
        "plt.xlim(min(angles), max(angles))\n",
        "\n",
        "# Estética mejorada\n",
        "plt.xticks(fontsize=15)\n",
        "plt.yticks(fontsize=15)\n",
        "ax = plt.gca()\n",
        "ax.spines['top'].set_visible(False)\n",
        "ax.spines['right'].set_visible(False)\n",
        "\n",
        "# Mostrar la gráfica\n",
        "plt.show()\n"
      ],
